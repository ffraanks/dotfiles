#!/bin/env bash

mpv_quality(){
mkdir -p $HOME/.config/mpv/scripts
mkdir -p $HOME/.config/mpv/script-opts
clear
cd $HOME/.config/mpv/scripts && wget -c "https://github.com/jgreco/mpv-youtube-quality/raw/master/youtube-quality.lua"
clear
cd $HOME/.config/mpv/script-opts && wget -c "https://github.com/jgreco/mpv-youtube-quality/raw/master/youtube-quality.conf"
clear
exit 0
}
mpv_quality
