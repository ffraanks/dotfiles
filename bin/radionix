#!/usr/bin/env sh
command -v mpg123 >/dev/null 2>&1 || { echo "Radionix depende do pacote 'mpg123' porém não está instalado! Instale o 'mpg123' e execute o Radionix novamente." >&2; exit 1; }
PLAYER="mpg123 --stereo --control --utf8 --title --preload 1 --buffer 1024"
 
MAIN() {
	clear
 cat << EOF
 +---[ RADIONIX ]---+
 
 -----
 01 - Alternativo
 02 - Blues
 03 - Clássicos
 04 - Country
 05 - Cyber/Geek
 06 - Dance
 07 - Flashback
 08 - Mpb
 09 - Pop
 10 - Rock
 11 - Samba/Pagode
 12 - Sertanejo
 13 - Notícias
 14 - Esporte
 -----
 
 * Digite h + ENTER para obter ajuda.
EOF
	read -r radio
	case $radio in
		01 | 1) ALTERNATIVO;;
		02 | 2) BLUES;;
		03 | 3) CLASSICOS;;
		04 | 4) COUNTRY;;
		05 | 5) CYBERGEEK;;
		06 | 6) DANCE;;
		07 | 7) FLASHBACK;;
		08 | 8) MPB;;
		09 | 9) POP;;
		10) ROCK;;
		11) SAMBAPAGODE;;
		12) SERTANEJO;;
		13) NOTICIAS;;
		14) ESPORTE;;
		h) HELP;;
		u) UPDATE;;
		q) EXIT;;
		*) echo "Opção incorreta"
           echo "Tente novamente. Digite h + ENTER para obter ajuda..."
           echo ""
           sleep 3
           clear
           MAIN
           ;;
esac
}
