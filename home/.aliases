# My aliases
  alias add='sudo pacman -S'
  alias up='sudo pacman -Syu'
  alias pkg='pacman -Ss'
  alias del='sudo pacman -Rns'
  alias vim='nvim'
  alias zshrc='$EDITOR ~/.zshrc && source ~/.zshrc'
  alias vk='VK_INSTANCE_LAYERS=VK_LAYER_MESA_overlay vkcube'
